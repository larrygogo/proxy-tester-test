{
    "build": {
      "beforeBuildCommand": "yarn build && yarn export",
      "beforeDevCommand": "yarn dev",
      "devPath": "http://localhost:3000",
      "distDir": "../out",
      "withGlobalTauri": false
    },
    "package": {
      "productName": "Proxy Tester",
      "version": "0.0.0"
    },
    "tauri": {
      "allowlist": {
        "all": true,
        "shell": {
          "all": false,
          "open": true
        },
        "window": {
          "all": true
        },
        "dialog": {
          "all": true
        }
      },
      "bundle": {
        "active": true,
        "icon": [
          "icons/32x32.png",
          "icons/128x128.png",
          "icons/128x128@2x.png",
          "icons/icon.icns",
          "icons/icon.ico"
        ],
        "identifier": "com.reversegame.proxy-tester",
        "targets": "all"
      },
      "security": {
        "csp": null
      },
      "updater": {
        "active": false
      },
      "windows": [
        {
          "fullscreen": false,
          "resizable": true,
          "title": "",
          "width": 800,
          "height": 600,
          "titleBarStyle": "Overlay"
        }
      ]
    }
}
