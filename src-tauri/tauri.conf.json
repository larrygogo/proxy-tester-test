{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run vite-build",
    "beforeDevCommand": "npm run vite-start",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Proxy Tester",
    "version": "../package.json"
  },
  "tauri": {
    "allowlist": {
      "os": {
        "all": true
      },
      "notification": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": ["*/**"]
      },
      "path": {
        "all": true
      },
      "window": {
        "all": true
      },
      "clipboard": {
        "all": true
      },
      "dialog": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": ["icons/icon.png", "icons/icon.icns", "icons/icon.ico"],
      "identifier": "ProxyTester",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://larrygogo.github.io/proxy-tester-test/install.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEU0RUVENzVGN0Q0QzhCQjQKUldTMGkweDlYOWZ1NU9mN0NkM0NEMWtIell0L24vYlNtK1g1R1FXcFI0RXZ3ekhBVEtZeTd3L0UK"
    },
    "windows": [
      {
        "decorations": false,
        "fullscreen": false,
        "resizable": true,
        "url": "/",
        "width": 1280,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ]
  }
}
